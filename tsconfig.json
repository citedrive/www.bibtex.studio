{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@lib/*": [
        "lib/*"
      ],
      "@locales/*": [
        "i18n/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "~assets/*": [
        "assets/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@config": [
        "util/config.ts"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}